package manager;

import model.Osoba;

public class OcenaManager {
	
	public static boolean ostaviKomentar(String brojZvezdicaStr,String komentar, String idStvariStr, String idOsobeStr){
		try {
			Integer brojZvezdica = Integer.parseInt(brojZvezdicaStr);
			Osoba osoba = OsobaManager.getOsobaById(Integer.parseInt(idOsobeStr));
			Stvari stvar = StvariManager.getStvarById(idStvariStr);
			
			return true;
		}catch(Exception e) {
			return false;
		}
	}
}
